{
  "name": "republik-frontend",
  "version": "1.0.0",
  "engines": {
    "node": "10.4.1"
  },
  "description": "",
  "main": "server/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --watch server --watch .env --watch lib/routes.js server/index.js",
    "start": "node server/index.js",
    "build": "next build",
    "heroku-postbuild": "npm run build",
    "tape": "cross-env NODE_ENV=test babel-tape-runner",
    "test": "cross-env NODE_ENV=test babel-tape-runner \"?(lib|pages|components)/**/*.test.js\" | faucet",
    "cloc": "find . -name '*.js' -not -path \"./node_modules*\" -not -path \"./.next*\" | xargs wc -l",
    "standardize": "standard --fix",
    "precommit": "lint-staged",
    "translations": "gsheets --key=1a-8RESf0Yjo2Mu15Q4SGWTmnY3ifuFbCVSD7WyYRhT0 --title=live --pretty --out lib/translations.json",
    "translations-vote": "gsheets --key=1oBlTiPKIon4p5aek5vEXluWMn0TRwrk-JftznGjTmdQ --title=live --pretty --out components/Vote/translations-vote.json",
    "meta-gallery": "gsheets --key=1lN4fGI7yHWoWewEbdpaRDWqysAOvO0Xtd1cdImIKG3Q --title=gallery --pretty --out lib/meta/gallery.json",
    "analyze": "BUNDLE_ANALYZE=both next build",
    "analyze:server": "BUNDLE_ANALYZE=server next build",
    "analyze:browser": "BUNDLE_ANALYZE=browser next build",
    "link:sg": "rm -rf .next && npm link @project-r/styleguide && rm -rf node_modules/glamor && ln -s @project-r/styleguide/node_modules/glamor node_modules/glamor && echo '\n⚠️  Make sure to restart the next.js server. To unlink run npm run unlink:sg.\n'",
    "unlink:sg": "rm -rf .next && rm -rf node_modules/glamor && rm -rf node_modules/@project-r/styleguide"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/orbiting/republik-frontend.git"
  },
  "author": "",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/orbiting/republik-frontend/issues"
  },
  "homepage": "https://github.com/orbiting/republik-frontend#readme",
  "lint-staged": {
    "*.js": [
      "standard --fix",
      "git add"
    ]
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "babel": {
    "plugins": [
      "markdown-in-js/lib/babel"
    ],
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ]
      },
      "test": {
        "presets": [
          [
            "next/babel",
            {
              "preset-env": {
                "modules": "commonjs",
                "targets": {
                  "node": true
                }
              }
            }
          ]
        ]
      }
    }
  },
  "devDependencies": {
    "abab": "^2.0.0",
    "babel-eslint": "^10.0.1",
    "babel-tape-runner": "^3.0.0",
    "cross-env": "^5.2.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "faucet": "0.0.1",
    "gsheets": "^2.0.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.4",
    "nodemon": "^1.18.7",
    "react-test-renderer": "^16.5.2",
    "standard": "^12.0.1",
    "tape": "^4.9.1"
  },
  "dependencies": {
    "@project-r/styleguide": "^6.29.1",
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "apollo-cache-inmemory": "^1.2.10",
    "apollo-client": "^2.4.2",
    "apollo-fetch": "^0.7.0",
    "apollo-link": "^1.2.3",
    "apollo-link-http": "^1.5.5",
    "apollo-link-ws": "^1.0.9",
    "compression": "^1.7.3",
    "cookie": "^0.3.1",
    "core-js": "^2.5.7",
    "d3-array": "^1.2.4",
    "d3-collection": "^1.0.7",
    "d3-dsv": "^1.0.10",
    "d3-ease": "^1.0.5",
    "d3-format": "^1.3.2",
    "d3-geo": "^1.11.3",
    "d3-hierarchy": "^1.1.8",
    "d3-interpolate": "^1.3.2",
    "d3-scale": "^2.1.2",
    "d3-shape": "^1.2.2",
    "d3-time-format": "^2.1.3",
    "d3-timer": "^1.0.9",
    "debug": "^3.2.5",
    "dotenv": "^4.0.0",
    "downshift": "^2.2.3",
    "express": "^4.16.3",
    "express-basic-auth": "^1.1.5",
    "glamor": "^2.20.40",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "helmet": "^3.13.0",
    "isomorphic-unfetch": "^3.0.0",
    "load-script": "^1.0.0",
    "lodash": "^4.17.11",
    "lru-cache": "^4.1.3",
    "markdown-in-js": "^1.1.4",
    "mdast-react-render": "^1.2.0",
    "next": "^7.0.2",
    "next-routes": "^1.4.2",
    "prop-types": "^15.6.2",
    "react": "^16.8.0",
    "react-apollo": "^2.2.4",
    "react-dom": "^16.8.0",
    "react-dropzone": "^4.3.0",
    "react-icons": "^2.2.7",
    "react-infinite-scroller": "^1.2.1",
    "react-maskedinput": "^4.0.1",
    "react-textarea-autosize": "^7.0.4",
    "subscriptions-transport-ws": "^0.9.15",
    "topojson": "^3.0.2",
    "uuid": "^3.3.2",
    "valid-url": "^1.0.9",
    "validator": "^9.4.1"
  }
}
